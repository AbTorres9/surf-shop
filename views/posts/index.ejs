<h1>Posts/index</h1>

<% posts.forEach(function(post){ %>
    <div>
        <h2><%=post.title %></h2>
        <div><p>Price:<%= post.price%></p></div>
        <div><%= post.description%></div>
        <div><%=post.location %></div>
        <div><a href="/posts/<%=post.id %>">View more</a></div>
    </div> 
<% }); %>
